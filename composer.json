{
  "name": "composer-at-heart/object-model",
  "description": "A handy way to create, control and cast php data objects",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "nesbot/carbon": "^2.61",
    "illuminate/support": "^9",
    "illuminate/validation": "^9.26"
  },
  "require-dev": {
    "pestphp/pest": "^1.21",
    "symfony/var-dumper": "^6.1"
  },
  "autoload": {
    "psr-4": {
      "CoderAtHeart\\ObjectModel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CoderAtHeart\\ObjectModel\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
